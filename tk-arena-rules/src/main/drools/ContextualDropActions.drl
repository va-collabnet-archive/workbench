rule "rule 1" 
#concept onto different SNOMED concept

   when
      (and
         (ViewFact(getView() == View.STATED))
         ($dropTarget: ConceptFact(this isKindOf Taxonomies.SNOMED, context == Context.DROP_TARGET))
         ($dropObject: ConceptFact(context == Context.DROP_OBJECT, getConcept().getTime() != Long.MAX_VALUE))
      )
   then
      actions.add(new CreateRelAction("Is-a", $dropTarget, Snomed.IS_A, $dropObject, config));
end

rule "rule 3" 
#concept onto NOT snomed concept

   when
      (and
         (ViewFact(getView() == View.STATED))
         ($dropTarget: ConceptFact(this not isKindOf Taxonomies.SNOMED, context == Context.DROP_TARGET))
         ($dropObject: ConceptFact(context == Context.DROP_OBJECT, getConcept().getTime() != Long.MAX_VALUE))
      )
   then
      actions.add(new CreateRelAction("is-a", $dropTarget, TermAux.IS_A, $dropObject, config));
end 


rule "rule 4" 
#concept onto same concept

   when
         ViewFact(getView() == View.STATED)
         $dropTarget: ComponentFact(context == Context.DROP_TARGET)
         $dropObject: ComponentFact(context == Context.DROP_OBJECT, component == $dropTarget.component,
            ((ComponentVersionBI)this.component).getTime() != Long.MAX_VALUE)
         not $groupConcept: RelGroupFact(context == Context.DROP_TARGET)
         not $conceptAttr: ConAttrFact(context == Context.DROP_TARGET)
         not $refex: RefexFact(context == Context.DROP_TARGET)
   then
      actions.add(new RetireAction("retire", $dropObject, config));
      actions.add(new CloneAction("clone", $dropObject, config));
      actions.add(new CloneAndRetireAction("clone & retire", $dropObject, config));
end

rule "rule 4.2" 
#CANCEL concept onto same concept

   when
         ViewFact(getView() == View.STATED)
         $dropTarget: ComponentFact(context == Context.DROP_TARGET)
         $dropObject: ComponentFact(((ComponentVersionBI)this.component).getTime() == Long.MAX_VALUE,
                context == Context.DROP_OBJECT, component == $dropTarget.component)
         not $refex: RefexFact(context == Context.DROP_TARGET)
         not $conceptAttr: ConAttrFact(context == Context.DROP_TARGET)
         not $relGroup: RelGroupFact(context == Context.DROP_TARGET)
   then
      actions.add(new CancelAction("cancel", $dropObject, vc, config));
end

rule "rule 4.3" 
#CANCEL conAttr 

   when
         ViewFact(getView() == View.STATED)
         $dropTarget: ConAttrFact(context == Context.DROP_TARGET)
         $dropObject: ConAttrFact(((ComponentVersionBI)this.component).getTime() == Long.MAX_VALUE,
                getConAttr().getPrimordialVersion().getTime() != Long.MAX_VALUE,
                context == Context.DROP_OBJECT, component == $dropTarget.component)
   then
      actions.add(new CancelAction("cancel", $dropObject, vc, config));
end


rule "rule 5" 
#template onto concept

   when
      (and
         (ViewFact(getView() == View.STATED))
         ($dropTarget: ConceptFact(context == Context.DROP_TARGET))
         ($dropObject: DescSpecFact(context == Context.DROP_OBJECT))
      )

   then
   	  if(DialectHelper.isTextForDialect($dropObject.getDescSpec().getDescriptionText(),
         		Language.EN_US.get(vc).getNid())){
         		actions.add(new AddFromDialectSpecAction("add GB dialect", $dropTarget, $dropObject,LANG_CODE.EN_GB, config));
         		}
      if(DialectHelper.isTextForDialect($dropObject.getDescSpec().getDescriptionText(),
         		Language.EN_UK.get(vc).getNid())){
         		actions.add(new AddFromDialectSpecAction("add US dialect", $dropTarget, $dropObject,LANG_CODE.EN_US, config));
                        }
end

rule "rule 6" 
#rel template onto concept

   when
      (and
         (ViewFact(getView() == View.STATED))
