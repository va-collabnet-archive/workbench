rule "rule 26" 
#procedure and decendants onto body structure

   when
      (and
         (ViewFact(getView() == View.STATED))
         ($dropTarget: ConceptFact(this isKindOf Snomed.PROCEDURE, context == Context.DROP_TARGET))
         ($dropObject: ConceptFact(this isKindOf Snomed.BODY_STRUCTURE, this not isKindOf Snomed.MORPHOLOGIC_ABNORMALITY,
         							 context == Context.DROP_OBJECT, getConcept().getTime() != Long.MAX_VALUE))
      )
   then
      actions.add(new CreateRelAction("Procedure Site", $dropTarget, Snomed.PROCEDURE_SITE, $dropObject, config));
      actions.add(new CreateRelAction("Procedure Site - Direct", $dropTarget, Snomed.PROCEDURE_SITE_DIRECT, $dropObject, config));
      actions.add(new CreateRelAction("Procedure Site - Indirect", $dropTarget, Snomed.PROCEDURE_SITE_INDIRECT, $dropObject, config));
end

rule "rule 27" 
#procedure onto morphologic abnormality
	when
	(and
