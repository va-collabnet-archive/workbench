<?xml version="1.0" encoding="UTF-8" ?> <!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.ihtsdo.qadb.data.DispositionStatus">
	
	<resultMap type="DispositionStatus" id="dispositionStatusResultMap">
		<id property="dispositionStatusUuid" column="disposition_status_uid"/>
		<result property="name" column="name"/>
	</resultMap>
	
	<select id="selectDispositionStatusByUUID" parameterType="String" resultMap="dispositionStatusResultMap">
		SELECT * FROM qa_disposition_status 
			WHERE 	disposition_status_uid=#{id}
	</select>
	
	<select id="selectAllDispositionStatuses" resultMap="dispositionStatusResultMap">
		select * from qa_disposition_status
	</select>
</mapper>