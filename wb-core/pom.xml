    <parent>
        <groupId>org.ihtsdo</groupId>
        <artifactId>wb-mmb</artifactId>
        <version>2.6.0-trek-94</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <artifactId>wb-core</artifactId>
    <name>${branch}: Workflow Architecture Components</name>
    <url> https://mgr.servers.aceworkspace.net/pbl/cubitci/pub/core/site/index.html </url>
    <dependencies>
        <dependency>
            <!-- An alternative to:
                <dependency>
                <groupId>javax.mail</groupId>
                <artifactId>mail</artifactId>
                <version>1.4</version>
                <scope>compile</scope>
                </dependency>
                secondary to licensing issues -->
            <groupId>org.apache.geronimo.javamail</groupId>
            <artifactId>geronimo-javamail_1.4_mail</artifactId>
            <version>1.6</version>
        </dependency>
        <dependency>
            <!-- An alternative to:
                <dependency>
                <groupId>javax.activation</groupId>
                <artifactId>activation</artifactId>
                <version>1.1</version>
                </dependency>
                secondary to licensing issues -->
            <groupId>org.apache.geronimo.specs</groupId>
            <artifactId>geronimo-activation_1.1_spec</artifactId>
            <version>1.0.2</version>
        </dependency>
        
        <!-- :BEGIN: Support for CollabNet -->
        <dependency>
			<groupId>org.apache.axis</groupId>
			<artifactId>axis</artifactId>
			<version>1.4</version>
		</dependency>
		<dependency>
			<groupId>commons-discovery</groupId>
			<artifactId>commons-discovery</artifactId>
			<version>0.4</version>
		</dependency>		
		<dependency>
			<groupId>org.mod4j.org.apache.commons</groupId>
			<artifactId>logging</artifactId>
			<version>1.0.4</version>
		</dependency>
		<dependency>
			<groupId>javax.xml</groupId>
			<artifactId>jaxrpc-api</artifactId>
			<version>1.1</version>
		</dependency>
        <dependency>
			<!-- sf_soap50_sdk.jar -->
			<groupId>com.collabnet</groupId>
			<artifactId>sf_soap50_sdk</artifactId>
			<version>5.0</version>
		</dependency>
        <!-- :END: Support for CollabNet -->
        
        <dependency>
            <groupId>org.ihtsdo</groupId>
            <artifactId>tk-concept-component</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.ihtsdo</groupId>
            <artifactId>wb-foundation</artifactId>
            <version>${project.version}</version>
            <scope>compile</scope>
        </dependency>
    </dependencies>
    <build>
      <plugins>
        <plugin>
         <groupId>org.apache.maven.plugins</groupId>
         <artifactId>maven-source-plugin</artifactId>
         <executions>
           <execution>
             <id>attach-sources</id>
             <goals>
               <goal>jar</goal>
             </goals>
           </execution>
         </executions>
        </plugin>
      </plugins>
    </build>
</project>
