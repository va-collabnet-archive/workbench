    <modelVersion>4.0.0</modelVersion>
    <groupId>au.gov.nehta</groupId>
    <artifactId>vodb</artifactId>
    <packaging>jar</packaging>
    <name>Versioned Object Database</name>
    <inceptionYear>2005</inceptionYear>
    <description>ACE editor toolkit and version-oriented database</description>
    <scm>
        <connection>scm:svn:https://csfe.aceworkspace.net/svn/repos/workbench/branches/nehta/vodb</connection>
        <developerConnection>scm:svn:https://csfe.aceworkspace.net/svn/repos/workbench/branches/nehta/vodb</developerConnection>
        <url>https://csfe.aceworkspace.net/sf/scm/do/listRepositories/projects.ace/scm</url>
    </scm>
    <licenses>
        <license>
            <name>Informatics, Inc. End User License Agreement</name>
        </license>
    </licenses>
    <dependencies>
        <dependency>
            <groupId>com.sleepycat</groupId>
            <artifactId>je</artifactId>
            <version>3.3.87</version>
        </dependency>
        <dependency>
            <groupId>au.gov.nehta</groupId>
            <artifactId>ace-api</artifactId>
        </dependency>
        <dependency>
            <groupId>org.apache.lucene</groupId>
            <artifactId>lucene-core</artifactId>
            <version>2.9.1</version>
        </dependency>
