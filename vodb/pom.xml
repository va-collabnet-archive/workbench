            <version>1.7.6</version>
        </dependency>
        <dependency>
            <groupId>org.dwfa</groupId>
            <artifactId>icons</artifactId>
            <version>1.14</version>
        </dependency>
        <dependency>
            <groupId>org.tmatesoft.svnkit</groupId>
            <artifactId>svnkit</artifactId>
            <version>${svnkit.version}</version>
        </dependency>
        <dependency>
            <!-- svnkit dependency -->
            <groupId>com.sun.jna</groupId>
            <artifactId>jna</artifactId>
            <version>3.0.5</version>
            <!--  <optional>true</optional>  -->
        </dependency>
        <dependency>
            <!-- svnkit dependency -->
            <groupId>com.trilead</groupId>
            <artifactId>trilead-ssh2</artifactId>
            <version>build213-svnkit-1.2-patch</version>
            <!--  <optional>true</optional>  -->
        </dependency>
        <dependency>
            <groupId>org.dwfa.jini</groupId>
            <artifactId>mahalo-dl</artifactId>
            <scope>compile</scope>
            <version>2.1</version>
        </dependency>
        <dependency>
            <groupId>org.dwfa.jini</groupId>
            <artifactId>outrigger-dl</artifactId>
            <scope>compile</scope>
            <version>2.1</version>
        </dependency>
        <dependency>
            <groupId>org.dwfa.jini</groupId>
            <artifactId>reggie-dl</artifactId>
            <scope>compile</scope>
            <version>2.1</version>
        </dependency>

        <dependency>
         <!-- An alternative to:
      <dependency>
          <groupId>javax.mail</groupId>
          <artifactId>mail</artifactId>
          <version>1.4</version>
          <scope>compile</scope>
      </dependency>
     secondary to licensing issues -->
            <groupId>org.apache.geronimo.javamail</groupId>
            <artifactId>geronimo-javamail_1.4_mail</artifactId>
            <version>1.6</version>
        </dependency>
        <dependency>
     <!-- An alternative to:
    <dependency>
      <groupId>javax.activation</groupId>
      <artifactId>activation</artifactId>
      <version>1.1</version>
    </dependency>
     secondary to licensing issues -->
