  <modelVersion>4.0.0</modelVersion>
  <groupId>org.dwfa</groupId>
  <artifactId>vodb</artifactId>
  <version>1.7</version>
  <packaging>jar</packaging>
  <name>Versioned Object Database</name>
  <url>https://mgr.cubit.aceworkspace.net/pbl/cubitci/pub/vodb/site</url>
  <inceptionYear>2005</inceptionYear>
  <description>ACE editor toolkit and version-oriented database</description>
  <distributionManagement>
    <repository>
      <uniqueVersion>false</uniqueVersion>
      <id>aceworkspace.net</id>
      <name>aceworkspace.net</name>
      <url>file:///ciweb/webapps/maven2</url>
      <layout>default</layout>
    </repository>
    <site>
      <id>aceworkspace.net</id>
      <url>file:///ciweb/webapps/vodb/site</url>
    </site>
  </distributionManagement>
  <scm>
    <connection>scm:svn:https://vodb.aceworkspace.net/svn/vodb/tags/vodb-1.7</connection>
    <developerConnection>scm:svn:https://vodb.aceworkspace.net/svn/vodb/tags/vodb-1.7</developerConnection>
    <url>https://vodb.aceworkspace.net/source/browse/vodb/tags/vodb-1.7</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>ace-api</artifactId>
      <version>1.7</version>
    </dependency>
    <dependency>
      <groupId>org.apache.lucene</groupId>
      <artifactId>lucene-core</artifactId>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>berkeleydb</groupId>
      <artifactId>je</artifactId>
      <version>3.2.74</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>icons</artifactId>
      <version>1.5</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>svn-javahl</artifactId>
      <version>1.4.4</version>
    </dependency>
    <dependency>
      <groupId>org.tmatesoft</groupId>
      <artifactId>svnkit</artifactId>
      <version>1.1.4.3248</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>mahalo-dl</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>outrigger-dl</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>reggie-dl</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>javax.activation</groupId>
      <artifactId>activation</artifactId>
      <version>1.1</version>
    </dependency>
    <dependency>
      <groupId>javax.mail</groupId>
      <artifactId>mail</artifactId>
      <version>1.4</version>
      <scope>compile</scope>
    </dependency>
    <!--
    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>log-dl</artifactId>
      <scope>compile</scope>
      <version>2.0.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>queue-dl</artifactId>
      <scope>compile</scope>
      <version>2.0.1</version>
    </dependency>
    -->
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>jsk-dl</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-lib</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-platform</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>browser</artifactId>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>jsk-policy</artifactId>
      <scope>provided</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>mahalo</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>outrigger</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>reggie</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>start</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>tools</artifactId>
      <scope>compile</scope>
      <version>2.1</version>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <workspace>/eclipse/workspace</workspace>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.dwfa</groupId>
        <artifactId>pbl-mojo</artifactId>
        <version>1.1</version>
        <configuration>
          <cubitProject>cubitci</cubitProject>
          <siteRoot>vodb</siteRoot>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
