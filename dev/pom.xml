<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <groupId>org.dwfa</groupId>
    <artifactId>dwfa-pom</artifactId>
    <version>2.0</version>
  </parent>

  <version>2.0.10</version>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.dwfa</groupId>
  <artifactId>foundation</artifactId>
  <packaging>jar</packaging>
  <name>Dwfa foundation module</name>
  <url>http://mgr.cubit.aceworkspace.net:8081/dwfa/foundation/</url>
  <inceptionYear>2004</inceptionYear>
  <organization>
    <name>Informatics, Inc.</name>
    <url>http://www.informatics.com</url>
  </organization>
  <issueManagement>
    <system>Scarab</system>
    <url>https://foundation.aceworkspace.net/servlets/Scarab</url>
  </issueManagement>
  <distributionManagement>
    <repository>
      <uniqueVersion>false</uniqueVersion>
      <id>aceworkspace.net</id>
      <name>aceworkspace.net</name>
      <url>file:///ciweb/webapps/maven2</url>
      <layout>default</layout>
    </repository>
    <site>
      <id>aceworkspace.net</id>
      <url>file:///ciweb/webapps/dwfa/foundation</url>
    </site>
  </distributionManagement>
  <scm>
    <connection>scm:svn:https://foundation.aceworkspace.net/svn/foundation/tags/foundation-2.0.10</connection>
    <developerConnection>scm:svn:https://foundation.aceworkspace.net/svn/foundation/tags/foundation-2.0.10</developerConnection>
    <url>https://foundation.aceworkspace.net/source/browse/foundation/tags/foundation-2.0.10</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-lib</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-platform</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <workspace>/eclipse/workspace</workspace>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <reporting>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-project-info-reports-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jxr-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
</project>