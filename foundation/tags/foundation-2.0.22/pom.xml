  <parent>
    <groupId>org.dwfa</groupId>
    <artifactId>dwfa-pom</artifactId>
    <version>2.3</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.dwfa</groupId>
  <artifactId>foundation</artifactId>
  <version>2.0.22</version>
  <packaging>jar</packaging>
  <name>DWFA foundation module</name>
  <url>https://mgr.servers.aceworkspace.net/pbl/cubitci/pub/foundation/site/index.html</url>
  <inceptionYear>2004</inceptionYear>
  <organization>
    <name>Informatics, Inc.</name>
    <url>http://www.informatics.com</url>
  </organization>
  <scm>
    <connection>scm:svn:https://csfe.aceworkspace.net/svn/repos/workbench/foundation/tags/foundation-2.0.22</connection>
    <developerConnection>scm:svn:https://csfe.aceworkspace.net/svn/repos/workbench/foundation/tags/foundation-2.0.22</developerConnection>
    <url>https://csfe.aceworkspace.net/sf/scm/do/listRepositories/projects.dwfa/scm/tags/foundation-2.0.22</url>
  </scm>
  <ciManagement>
    <system>continuum</system>
    <url>https://mgr.servers.aceworkspace.net/apps/ace_continuum2/continuum/projectView.action?projectId=1</url>
  </ciManagement>
  <dependencies>
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-lib</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-platform</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-release-plugin</artifactId>
        <version>2.0-beta-7</version>
        </plugin>
        <plugin>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <workspace>/eclipse/workspace</workspace>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.dwfa</groupId>
        <artifactId>pbl-mojo</artifactId>
        <version>1.1</version>
        <configuration>
          <cubitProject>cubitci</cubitProject>
          <siteRoot>foundation</siteRoot>
          <pluginsInMetadata>
            <plugin>dwfa-mojo</plugin>
            <plugin>term-mojo</plugin>
            <plugin>pbl-mojo</plugin>
          </pluginsInMetadata>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <reporting>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-project-info-reports-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jxr-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
</project>
