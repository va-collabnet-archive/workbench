<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.dwfa.mojo.export</groupId>
	<artifactId>AceExportTest</artifactId>
	<packaging>jar</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>AceExportTest</name>
	<url>http://maven.apache.org</url>	
		<properties>
		<out.dir>D:/Data/ATestXML/test/Terminology/Export</out.dir>
		 </properties>
	
	<dependencies>
		<dependency>
			<groupId>org.dwfa.mojo</groupId>
			<artifactId>exporter</artifactId>
			<version>0.0.1-SNAPSHOT</version>
			<scope>runtime-directory</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.dwfa.mojo</groupId>
				<artifactId>exporter</artifactId>
				<version>0.0.1-SNAPSHOT</version>
				<dependencies>
					<dependency>
						<groupId>org.dwfa</groupId>
						<artifactId>ace-api</artifactId>
						<version>1.33-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>org.dwfa</groupId>
						<artifactId>vodb</artifactId>
						<version>1.39-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>org.dwfa</groupId>
						<artifactId>term-mojo</artifactId>
						<version>1.43-SNAPSHOT</version>
					</dependency>
				</dependencies>
				<executions>
					<execution>
						<id>aceExport</id>
						<phase>process-resources</phase>
						<goals>
							<goal>aceExport</goal>
						</goals>
						<configuration>
							<dbPath>D:/Data/ATestXML/test/Terminology/Ace/ace-bundle.dir-2010-02-25/ace-bundle.dir/berkeley-db</dbPath>
							<publishers>
								<publisherDTO>
									<name>ReadV2</name>
									<id_uuidS>17b9058f-8af8-3d1a-b750-b23d94eed52b</id_uuidS>
									<publisherClassName>org.dwfa.mojo.exporter.publishers.ReadV2Publisher</publisherClassName>
									<outputDirectory>${out.dir}</outputDirectory>
									<configProps>
										<property>
											<name>Peter</name>
											<value>Pan</value>
										</property>
									</configProps>
								</publisherDTO>
								<publisherDTO>
									<name>ReadV3</name>
									<id_uuidS>7f951a3a-39e0-3fba-92a0-1138edafa4c0</id_uuidS>
									<publisherClassName>org.dwfa.mojo.exporter.publishers.ReadV3Publisher</publisherClassName>
									<outputDirectory>${out.dir}</outputDirectory>
									<configProps>
										<property>
											<name>Peter</name>
											<value>Pan</value>
										</property>
									</configProps>
								</publisherDTO>
								<publisherDTO>
									<name>SnoMed</name>
									<id_uuidS>0418a591-f75b-39ad-be2c-3ab849326da9</id_uuidS>
									<publisherClassName>org.dwfa.mojo.exporter.publishers.SnoMedPublisher</publisherClassName>
									<outputDirectory>${out.dir}</outputDirectory>
									<configProps>
										<property>
											<name>Peter</name>
											<value>Pan</value>
										</property>
									</configProps>
								</publisherDTO>

							</publishers>


						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
