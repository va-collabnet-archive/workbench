  <parent>
    <groupId>org.dwfa</groupId>
    <artifactId>dwfa-pom</artifactId>
    <version>2.0</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.dwfa</groupId>
  <artifactId>term-mojo</artifactId>
  <version>1.15</version>
  <packaging>maven-plugin</packaging>
  <name>ACE mojo</name>
  <url>https://mgr.cubit.aceworkspace.net/pbl/cubitci/pub/ace-mojo/site/</url>
  <distributionManagement>
    <repository>
      <uniqueVersion>false</uniqueVersion>
      <id>aceworkspace.net</id>
      <name>aceworkspace.net</name>
      <url>file:///ciweb/webapps/maven2</url>
      <layout>default</layout>
    </repository>
    <site>
      <id>aceworkspace.net</id>
      <url>file:///ciweb/webapps/ace-mojo/site</url>
    </site>
  </distributionManagement>
  <scm>
    <connection>scm:svn:https://term-mojo.aceworkspace.net/svn/term-mojo/tags/term-mojo-1.15</connection>
    <developerConnection>scm:svn:https://term-mojo.aceworkspace.net/svn/term-mojo/tags/term-mojo-1.15</developerConnection>
    <url>https://term-mojo.aceworkspace.net/source/browse/term-mojo/tags/term-mojo-1.15</url>
  </scm>
  <organization>
    <name>Informatics, Inc.</name>
    <url>http://www.informatics.com</url>
  </organization>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <inceptionYear>2006</inceptionYear>
  <description>This project provides a set of maven goals that are used to
    transform terminology data in 
  preperation for importing into standard file formats used by the ACE version-orented database, and by other applications. 
  </description>
  <dependencies>
     <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>dwfa-mojo</artifactId>
       <version>2.0.10</version>
     </dependency>
    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>vodb</artifactId>
      <version>1.16</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven</groupId>
      <artifactId>maven-plugin-api</artifactId>
      <version>2.0.6</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven</groupId>
      <artifactId>maven-core</artifactId>
      <version>2.0.6</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven</groupId>
      <artifactId>maven-model</artifactId>
      <version>2.0.6</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.derby</groupId>
      <artifactId>derby</artifactId>
      <version>10.3.1.4</version>
    </dependency>
    <dependency>
      <groupId>berkeleydb</groupId>
      <artifactId>je</artifactId>
      <version>3.2.74</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven</groupId>
      <artifactId>maven-project</artifactId>
      <version>2.0.6</version>
    </dependency>
    <dependency>
      <groupId>org.apache.lucene</groupId>
      <artifactId>lucene-core</artifactId>
      <version>2.2.0</version>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <workspace>/eclipse/workspace</workspace>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.dwfa</groupId>
        <artifactId>pbl-mojo</artifactId>
        <version>1.1</version>
        <configuration>
          <cubitProject>cubitci</cubitProject>
          <siteRoot>ace-mojo</siteRoot>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <reporting>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-plugin-plugin</artifactId>
      </plugin>      
    </plugins>
  </reporting>
</project>
