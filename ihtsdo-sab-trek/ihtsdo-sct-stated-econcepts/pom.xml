<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.ihtsdo</groupId>
	<artifactId>ihtsdo-sct-stated-econcepts</artifactId>
	<version>2010.01.31-SNAPSHOT</version>
	<name>Convert SNOMED Stated and Inferred to eConcepts</name>
	<description>Convert SNOMED SCT RF1 Stated and Inferred release to eConcepts</description>
	<scm>
		<connection>scm:svn:https://csfe.aceworkspace.net/svn/repos/wb-refset-sab/trunk/wb-refset-sab/ihtsdo-sct-stated-econcepts</connection>
		<developerConnection>scm:svn:https://csfe.aceworkspace.net/svn/repos/wb-refset-sab/trunk/wb-refset-sab/ihtsdo-sct-stated-econcepts</developerConnection>
		<url>https://csfe.aceworkspace.net/integration/viewvc/viewvc.cgi/trunk/wb-refset-sab/ihtsdo-sct-stated-econcepts/?root=wb-refset-sab&amp;system=exsy1002</url>
	</scm>
	<properties>
		<org.ihtsdo.wb-gui.version>2.6.0-trek-2-SNAPSHOT</org.ihtsdo.wb-gui.version>
		<org.ihtsdo.wb-mojo.version>${org.ihtsdo.wb-gui.version}</org.ihtsdo.wb-mojo.version>
		<org.ihtsdo.wb-bdb.version>${org.ihtsdo.wb-gui.version}</org.ihtsdo.wb-bdb.version>
	</properties>
	<dependencies>
		<dependency>
			<groupId>org.ihtsdo</groupId>
			<artifactId>sct-stated</artifactId>
			<!-- 2009.01.31_2010.01.31 -->
			<!-- 2010.01.31_2010.01.31 -->
			<version>2009.01.31_2010.01.31</version>
			<scope>runtime-directory</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.ihtsdo</groupId>
				<artifactId>wb-mojo</artifactId>
				<version>${org.ihtsdo.wb-mojo.version}</version>
				<executions>
					<execution>
						<id>write-directories</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>write-directories</goal>
						</goals>
						<configuration>
							<targetSubDir>generated-sources</targetSubDir>
						</configuration>
					</execution>
					<execution>
						<id>sct-si-to-econcepts</id>
						<phase>process-sources</phase>
						<goals>
							<goal>sct-si-to-econcepts</goal>
						</goals>
						<configuration>
							<targetSubDir>generated-sources</targetSubDir>
							<sctInputDirArray>
								<param>/org/snomed/</param>
							</sctInputDirArray>
							<includeCTV3ID>true</includeCTV3ID>
							<includeSNOMEDRTID>true</includeSNOMEDRTID>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<distributionManagement>
		<snapshotRepository>
			<id>maestro_ihtsdo.wb-snapshots</id>
			<name>IHTSDO Workbench Snapshots Repository</name>
			<url>http://mgr.servers.aceworkspace.net/apps/maestro_ihtsdo/archiva/repository/wb-snapshots/</url>
		</snapshotRepository>
		<repository>
			<id>maestro_ihtsdo.wb-releases</id>
			<name>IHTSDO Workbench Releases Repository</name>
			<url>http://mgr.servers.aceworkspace.net/apps/maestro_ihtsdo/archiva/repository/wb-releases/</url>
		</repository>
	</distributionManagement>
</project>