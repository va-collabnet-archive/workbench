<?xml version="1.0" encoding="UTF-8"?>
<component>
    <fileSets>
        <fileSet>
            <directory>target/generated-resources/</directory>
            <lineEnding>keep</lineEnding>
            <filtered>false</filtered>
            <useStrictFiltering>false</useStrictFiltering>
            <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory>/</outputDirectory>
            <excludes>
                <exclude>config/ace.config</exclude>
              <!-- TODO: check this, may just have been an oversight before -->
                <exclude>processes/**</exclude>
              <exclude>profiles/**</exclude>
              <exclude>queues/**</exclude>
              <exclude>berkely-db/mutable/**</exclude>
              <exclude>**/TestForEdgCType2Req.task</exclude>
              <exclude>**/TestForEdgEType2Req.task</exclude>
            </excludes>
            <fileMode>0644</fileMode>
            <directoryMode>0755</directoryMode>
        </fileSet>
        <fileSet>
            <directory>src/main/config</directory>
            <lineEnding>keep</lineEnding>
            <filtered>false</filtered>
            <useStrictFiltering>false</useStrictFiltering>
            <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory>config</outputDirectory>
            <fileMode>0644</fileMode>
            <directoryMode>0755</directoryMode>
        </fileSet>
        <fileSet>
            <directory>src/main/queues</directory>
            <lineEnding>keep</lineEnding>
            <filtered>false</filtered>
            <useStrictFiltering>false</useStrictFiltering>
            <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory>queues</outputDirectory>
            <fileMode>0644</fileMode>
            <directoryMode>0755</directoryMode>
        </fileSet>
        <fileSet>
            <directory>src/main/docs</directory>
            <lineEnding>keep</lineEnding>
            <filtered>false</filtered>
            <useStrictFiltering>false</useStrictFiltering>
            <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory>docs</outputDirectory>
            <fileMode>0644</fileMode>
            <directoryMode>0755</directoryMode>
        </fileSet>
        <fileSet>
            <directory>src/main/plugins/component/</directory>
            <lineEnding>keep</lineEnding>
            <filtered>false</filtered>
            <useStrictFiltering>false</useStrictFiltering>
            <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory>plugins/component/</outputDirectory>
            <fileMode>0644</fileMode>
            <directoryMode>0755</directoryMode>
        </fileSet>
        <!-- Needed for collabnet tracker integration -->
        <fileSet>
            <directory>src/main/cacerts</directory>
            <lineEnding>keep</lineEnding>
            <filtered>false</filtered>
            <useStrictFiltering>false</useStrictFiltering>
            <useDefaultExcludes>true</useDefaultExcludes>
            <outputDirectory>config</outputDirectory>
            <includes>
                <include>cacerts</include>
            </includes>
            <fileMode>0644</fileMode>
            <directoryMode>0755</directoryMode>
        </fileSet>
        <!-- End for collabnet tracker integration -->
    </fileSets>
</component>
