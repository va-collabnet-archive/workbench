  <parent>
    <groupId>org.dwfa</groupId>
    <artifactId>dwfa-pom</artifactId>
    <version>2.3</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.dwfa</groupId>
  <artifactId>ace-api</artifactId>
  <version>1.31.7</version>
  <packaging>jar</packaging>
  <name>ACE API</name>
  <url>https://mgr.servers.aceworkspace.net/pbl/cubitci/pub/ace-api/site/index.html</url>
  <scm>
    <connection>scm:svn:https://csfe.aceworkspace.net/svn/repos/workbench/ace-api/tags/ace-api-1.31.7</connection>
    <developerConnection>scm:svn:https://csfe.aceworkspace.net/svn/repos/workbench/ace-api/tags/ace-api-1.31.7</developerConnection>
    <url>https://csfe.aceworkspace.net/sf/scm/do/listRepositories/projects.ace/scm/tags/ace-api-1.31.7</url>
  </scm>
  <ciManagement>
    <system>continuum</system>
    <url>https://mgr.servers.aceworkspace.net/apps/ace_continuum2/continuum/groupSummary.action</url>
  </ciManagement>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>core</artifactId>
      <version>2.0.19.2</version>
    </dependency>
    <!-- added by lawley to support drag-and-drop -->
<!-- 
    This creates a circular dependency, and we can't release 
    while this dependency remains...

    <dependency>
      <groupId>org.dwfa</groupId>
      <artifactId>vodb</artifactId>
      <version>1.20</version>
      <scope>compile</scope>
    </dependency>
 -->
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-lib</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>net.jini</groupId>
      <artifactId>jsk-platform</artifactId>
      <version>2.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.dwfa.jini</groupId>
      <artifactId>browser</artifactId>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>javax.activation</groupId>
      <artifactId>activation</artifactId>
      <version>1.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>javax.mail</groupId>
      <artifactId>mail</artifactId>
      <version>1.4</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.lucene</groupId>
      <artifactId>lucene-core</artifactId>
      <version>2.3.1</version>
    </dependency>
    <dependency>
      <groupId>org.tmatesoft.svnkit</groupId>
      <artifactId>svnkit-javahl</artifactId>
      <version>1.2.1.5297</version>
    </dependency>
    <dependency>
      <groupId>org.jmock</groupId>
      <artifactId>jmock</artifactId>
      <version>2.5.1</version>
      <scope>test</scope>
      <type>jar</type>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <workspace>/eclipse/workspace</workspace>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.dwfa</groupId>
        <artifactId>pbl-mojo</artifactId>
        <version>1.1</version>
        <configuration>
          <cubitProject>cubitci</cubitProject>
          <siteRoot>ace-api</siteRoot>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
